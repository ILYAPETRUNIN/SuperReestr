.custom-table
   container(true)
   flexy(center,center,nowrap,column)
   max-width 95% !important
   &__actions
      width 100%
      margin 20px 0px
      flexy(flex-start,flex-start,wrap)
      &_btn
         flexy(center,center)
         margin-right 10px
   &__table
      width 100%
      max-height: 800px !important
   &__filter
      width 100%
      flexy(space-between,flex-start,wrap)
      margin 30px 0px
      &_btn
         margin 0px 10px
      &_btns
         margin-top 30px